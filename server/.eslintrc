{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "max-len": ["error", {
      "ignoreComments": true,
      "ignoreUrls": true,
      "code": 140
    }],
    "camelcase": 0,
    "no-prototype-builtins": 0,
    "no-console": 0,
    "strict": 0,
    "consistent-return": 0,
    "no-unused-expressions": 0,
    "indent": [2, 2],
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "import/no-extraneous-dependencies": 0,
    "no-use-before-define": 0,
    "no-return-assign": 0,
    "space-before-function-paren": [2, "always"],
    "no-restricted-globals": 0,
    "func-names": 0
  }
}